<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
</head>

<body>
  <h1>
    <%= message %>
  </h1>
  <% if (showItems) { %>
    <ul>
      <% for (var i=0; i < items.length; i++) { %>
        <li>
          <%= items[i] %>
        </li>
        <% } %>
    </ul>
    <% } %>
</body>

</html>